{
  "presets": [
    [
      "@babel/env",
      {
        "targets": "current node",
        "modules": "cjs"
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
  ]
}